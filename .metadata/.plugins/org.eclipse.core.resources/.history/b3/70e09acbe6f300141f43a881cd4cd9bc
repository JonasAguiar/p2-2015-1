
public class Pessoa {
	
	private String nome;
	
	public Pessoa(String nome) {
		this.nome = nome;
	}
	
	public String getNome() {
		return this.nome;
	}
	
	@Override
	public String toString() {
		return nome; 
	}
	
	@Override
	public boolean equals(Object outro) {
		return true; 
	}

}
