import java.util.HashMap;


public class FrequenciaDeNomes {

	public static void main(String[] args) {
		
		HashMap<String, Integer> frequenciaDeNomes = new HashMap<String, Integer>();
		
		for (int i = 0; i < args.length; i++) {
			if (frequenciaDeNomes.get(args[i]) == null) {
				frequenciaDeNomes.put(args[i], 1);
			} else {
				frequenciaDeNomes.put(args[i], frequenciaDeNomes.get(args[i]) + 1);
			}
		}		
	}
}
