package aulaHeranca;


public class Programador extends Funcionario {
	
	private Linguagem linguagem;

	public Programador(String nome, Linguagem linguagem) throws Exception {
		super(nome);
		this.linguagem = linguagem;
		
	}
	
	public Linguagem getLinguagem() {
		return linguagem;
	}

	enum Linguagem {
		Java, Python
	}
}
