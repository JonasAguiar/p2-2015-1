package aulaHeranca;

import static org.junit.Assert.*;

import java.util.HashSet;

import org.junit.Test;

public class FuncionarioTest {

	@Test
	public void test() {
		Funcionario programador = new Funcionario("Matteus","Programador","Java");
		assertEquals("Matteus", programador.getNome());
		assertEquals("Programador", programador.getCargo());
		assertEquals("Java",programador.getLinguagem());
	
	}

}
