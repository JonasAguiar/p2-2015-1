import java.util.HashSet;


public class PessoasDuplicadas {
	
	public static void main(String[] args) {
		
		HashSet<Pessoa> nomesDistintos = new HashSet<Pessoa>();
		HashSet<Pessoa> nomesDuplicados = new HashSet<Pessoa>();
		
		for (int i = 0; i < args.length; i++) {
			Pessoa pessoa = new Pessoa(args[i]);
			
			if (!nomesDistintos.add(pessoa) {
				nomesDuplicados.add(args[i]);
			}				
		}
		nomesDistintos.removeAll(nomesDuplicados);
		
		System.out.println(nomesDistintos);
		System.out.println(nomesDuplicados);
	}

}
